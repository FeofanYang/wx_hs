/* 分页器 */

.pagination {
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 80px;
  z-index: 5;
}

.pagination view, .pagination button,.pagination picker {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 80rpx;
  border: none;
  color: #fff;
  background-color: #1d3657;
  box-shadow: 1px 1px 5px #1d3657;
}

.filterBtn {
  width: 80rpx;
}

.filterBtn text{
  position: absolute;
  left: 50%;
  transform: translate(-50%, 0);
}

.pagination button[disabled]:not([type]) {
  opacity: 0.5;
  color: #fff;
  background-color: #1d3657;
}

.pagination view {
  padding: 0 20px;
}
